<HBox alignment="CENTER_LEFT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minWidth="190.0" prefHeight="35.0" prefWidth="329.0" spacing="5.0" styleClass="sequence-item-container" stylesheets="@../../../styles/default-theme.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.xj.gui.controllers.content.program.SequenceItemBindMode">
   <children>
      <Text fx:id="sequenceName" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="New Sequence" HBox.hgrow="ALWAYS">
         <font>
            <Font size="16.0" />
         </font>
      </Text>
      <Button fx:id="addMemeButton" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="28.0" prefWidth="32.0" text=" " HBox.hgrow="ALWAYS">

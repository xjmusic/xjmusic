            <ImageView fitHeight="19.0" fitWidth="18.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@/icons/plus.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <HBox fx:id="memeHolder" alignment="CENTER_LEFT" prefHeight="29.0" prefWidth="181.0" HBox.hgrow="ALWAYS" />

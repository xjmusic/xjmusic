<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<?import javafx.scene.layout.Pane?>
<?import javafx.scene.control.Label?>
<BorderPane fx:id="mainBorderPane" styleClass="sequence-binding-item-container" stylesheets="@/styles/default-theme.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.xj.gui.controllers.content.program.SequenceBindingItemController">
   <center>
      <Label fx:id="sequenceName" text="Text">
         <font>
            <Font size="14.0"/>
         </font>
         <HBox.margin>
            <Insets left="5.0" right="5.0"/>
         </HBox.margin>
      </Label>
   </center>
   <right>
      <Button fx:id="deleteSequence" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="28.0" prefWidth="27.0" styleClass="sequence-item-button" text=" " BorderPane.alignment="CENTER">
         <graphic>
            <ImageView fitHeight="19.0" fitWidth="18.0" pickOnBounds="true" preserveRatio="true" translateX="-4.0">
               <Image url="@/icons/delete.png"/>
            </ImageView>
         </graphic>
      </Button>
   </right>
   <bottom>
      <Pane fx:id="sequenceBindingMemeContainer"/>
   </bottom>
   <padding>
      <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
   </padding>
</BorderPane>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.TableView?>
<TabPane
  fx:controller="io.xj.gui.controllers.content.ContentBrowserController"
  fx:id="container"
  xmlns="http://javafx.com/javafx"
  xmlns:fx="http://javafx.com/fxml"
  AnchorPane.topAnchor="0"
  AnchorPane.rightAnchor="0"
  AnchorPane.bottomAnchor="0"
  AnchorPane.leftAnchor="0"
  tabClosingPolicy="UNAVAILABLE">
  <Tab text="Libraries"
       fx:id="librariesTab">
    <TableView
      fx:id="librariesTable"
      xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      AnchorPane.topAnchor="0"
      AnchorPane.rightAnchor="0"
      AnchorPane.bottomAnchor="0"
      AnchorPane.leftAnchor="0"
      maxHeight="Infinity"
      maxWidth="Infinity"/>
  </Tab>
  <Tab text="Programs"
       fx:id="programsTab">
    <TableView
      fx:id="programsTable"
      xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      AnchorPane.topAnchor="0"
      AnchorPane.rightAnchor="0"
      AnchorPane.bottomAnchor="0"
      AnchorPane.leftAnchor="0"
      maxHeight="Infinity"
      maxWidth="Infinity"/>
  </Tab>
  <Tab text="Instruments"
       fx:id="instrumentsTab">
    <TableView
      fx:id="instrumentsTable"
      xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      AnchorPane.topAnchor="0"
      AnchorPane.rightAnchor="0"
      AnchorPane.bottomAnchor="0"
      AnchorPane.leftAnchor="0"
      maxHeight="Infinity"
      maxWidth="Infinity"/>
  </Tab>
</TabPane>

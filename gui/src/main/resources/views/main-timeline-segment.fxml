<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox styleClass="main-timeline-segment" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.xj.gui.controllers.MainTimelineSegmentController">
   <HBox prefHeight="100.0" prefWidth="200.0">
      <children>
        <VBox prefHeight="200.0" prefWidth="100.0">
          <Label styleClass="label" text="Offset" />
          <Text fx:id="offsetText" styleClass="value" text="2097" />
        </VBox>
        <VBox layoutX="10.0" layoutY="10.0" prefHeight="200.0" prefWidth="100.0">
          <Label fx:id="labelType" styleClass="label" text="Type" />
          <Text fx:id="deltaText" styleClass="value" text="32" />
        </VBox>
      </children>
   </HBox>
   <HBox prefHeight="100.0" prefWidth="200.0">
      <children>
         <VBox prefHeight="100.0" prefWidth="41.0">
            <children>
             <Label styleClass="label" text="Total" />
             <Text fx:id="totalText" styleClass="value" text="16" />
            </children>
         </VBox>
         <VBox prefHeight="100.0" prefWidth="55.0">
            <children>
             <Label styleClass="label" text="Density" />
             <Text fx:id="densityText" styleClass="value" text="0.62" />
            </children>
         </VBox>
         <VBox prefHeight="100.0" prefWidth="52.0">
            <children>
             <Label styleClass="label" text="Tempo" />
             <Text fx:id="tempoText" styleClass="value" text="75" />
            </children>
         </VBox>
         <VBox prefHeight="100.0" prefWidth="64.0">
            <children>
             <Label styleClass="label" text="Key" />
             <Text fx:id="keyText" styleClass="value" text="C minor" />
            </children>
         </VBox>
      </children>
   </HBox>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
